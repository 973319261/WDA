<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" >
    <!--按钮边框-->
    <!--solid用以指定内部填充色-->
    <solid android:color="#F3F1F1"/>
    <!--可以定义描边的宽度，颜色，虚实线等-->
    <stroke
        android:width="1dp"
        android:color="#F3F1F1" />
    <!--圆角-->
    <corners
        android:bottomLeftRadius="10dp"
        android:bottomRightRadius="10dp"/>
</shape>
